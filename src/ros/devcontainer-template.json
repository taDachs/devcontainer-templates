{
    "id": "ros",
    "version": "0.0.2",
    "name": "ROS",
    "description": "A template for setting up a container with robot-folders",
    "documentationURL": "https://github.com/devcontainers/template-starter/tree/main/src/color",
    "licenseURL": "https://github.com/devcontainers/template-starter/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "ROS version",
            "proposals": [
                "rolling-desktop-full",
                "jammy-desktop-full",
                "humble-desktop-full",
                "noetic-desktop-full"
            ],
            "default": "humble-desktop-full"
        },
        "additionalAptPackages": {
            "type": "string",
            "description": "Additional apt packages to install",
            "default": ""
        },
        "username": {
            "type": "string",
            "description": "Username for remote user",
            "proposals": [
                "robot",
                "ros"
            ],
            "default": "robot"
        }
    },
    "platforms": [
        "Any"
    ]
}
